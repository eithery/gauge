<?xml version="1.0"?>
<!--
  [dbo].[emails]
  Contains email address records.
-->
<table name="emails">
  <columns>
    <col id="true"/>
    <col name="parent_id" ref="primaryReps" required="true"/>
    <col name="email" length="256" required="true"/>
    <col name="start_date" type="datetime" required="true"/>
    <col name="end_date" type="datetime"/>
    <col name="is_primary" required="true"/>
  </columns>
</table>
  