<?xml version="1.0"?>
<!--
  [dbo].[repStatuses]
  Contains available rep statuses.
-->
<table name="repStatuses">
  <columns>
    <col name="name" length="256" required="true"/>
    <col name="display_name" length="256" required="true"/>
    <col name="is_enabled" required="true" default="true"/>
    <col name="order" type="int" required="true"/>
    <col name="metadata" type="xml"/>
  </columns>
</table>
