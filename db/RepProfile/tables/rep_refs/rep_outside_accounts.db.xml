<?xml version="1.0"?>
<!--
  [dbo].[repOutsideAccounts]
  Contains outside accounts for reps.
-->
<table name="repOutsideAccounts">
  <columns>
    <col name="repId" ref="primaryReps"/>
    <col name="accountNumber" length="20" required="true"/>
    <col name="opened" type="datetime"/>
    <col name="closed" type="datetime"/>
    <col name="brokerName" length="256"/>
    <col name="brokerAttentionName" length="256"/>
    <col name="brokerAddressId" ref="addresses"/>
    <col name="notes"/>
    <col name="registration" length="256"/>
    <col name="registrationExt" length="256"/>
    <col name="letterCreated" type="datetime"/>
    <col name="letterCreatedBy" length="256"/>
    <col name="letterSent" type="datetime"/>
    <col name="letterNotes"/>
    <col name="updated" type="datetime"/>
    <col name="updatedBy" length="256"/>
  </columns>
</table>
