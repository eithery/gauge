<?xml version="1.0"?>
<!--
  [dbo].[repOutsideBusiness]
  Contains outside business information for reps.
-->
<table name="repOutsideBusiness">
  <columns>
    <col name="repId" ref="primaryReps"/>
    <col name="businessName" length="256" required="true"/>
    <col name="startDate"/>
    <col name="endDate"/>
    <col name="verified" type="datetime"/>
    <col name="verifiedBy" length="256"/>
    <col name="notes"/>
  </columns>
</table>
