<?xml version="1.0"?>
<!--
  [crd].[finra_feeds]
  Contains FINRA feeds.
-->
<table name="finra_feeds" schema="crd">
  <columns>
    <col id="true"/>
    <col name="name" length="256" required="true"/>
    <col name="display_name" length="256" required="true"/>
    <col name="posted_date_tag" length="256" required="true"/>
    <col name="argument_tag" length="256" required="true"/>
    <col name="feed_scope" type="enum" required="true" check="0..1"/>
  </columns>
</table>
