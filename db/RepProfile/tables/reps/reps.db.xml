<?xml version="1.0"?>
<!--
  [dbo].[reps]
  Contains all reps.
-->
<table name="reps">
  <columns>
    <col name="code" length="10" required="true" unique="true" business_id="true"/>
    <col name="name" length="256"/>
    <col name="officeId" ref="offices" required="true"/>
    <col name="description"/>
    <timestamps case="camel"/>
  </columns>
</table>
