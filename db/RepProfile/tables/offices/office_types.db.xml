<?xml version="1.0"?>
<!--
  [dbo].[office_types]
  Contains available office types.
-->
<table name="office_types">
  <columns>
    <col id="true"/>
    <col name="name" length="256" required="true" unique="true"/>
    <col name="display_name" length="256" required="true"/>
    <col name="description"/>
    <col name="is_bank_branch" required="true"/>
    <col name="allow_primary_reps" required="true" default="true"/>
    <col name="allow_secondary_reps" required="true" default="true"/>
    <col name="allow_roles" required="true" default="true"/>
    <col name="allow_splits" required="true" default="true"/>
    <col name="allowed_suboffices" type="xml" required="true"/>
  </columns>
</table>
