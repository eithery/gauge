<?xml version="1.0"?>
<!--
  [dbo].[offices]
  Contains offices hierarchy.
-->
<table name="offices">
  <columns>
    <col name="parentId" ref="offices"/>
    <col ref="office_types" required="true"/>
    <col name="treeLevel" type="enum" required="true"/>
    <col name="code" length="10" required="true" unique="true" business_id="true"/>
    <col name="tax_id" length="9" check="len(9)"/>
    <col name="crd_number" length="10"/>
    <col name="name" length="256" required="true"/>
    <col name="description"/>
    <col name="addressId" ref="addresses"/>
    <col name="mailingAddressId" ref="addresses"/>
    <col name="contactsId" ref="contacts"/>
    <col name="is_broker_dealer" required="true"/>
    <timestamps case="camel"/>
  </columns>
</table>
