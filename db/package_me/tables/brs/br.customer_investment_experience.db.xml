<?xml version="1.0"?>
<!--
	[br].[customer_investment_experience]
	Contains customer investment experience values.
-->
<table name="customer_investment_experience" schema="br">
	<columns>
		<col name="customer_id" ref="br_natural_owner" business_id="true"/>
		<col name="investment_experience_id" ref="ref.investment_experience" business_id="true"/>
		<col name="experience_level_id" ref="ref.investment_experience_levels"/>
		<col name="years_level_id" ref="ref.experience_years_levels"/>
		<col name="years_amount" type="byte" check=">0"/>
		<col name="transactions_level_id" ref="ref.transactions_per_year_levels"/>
		<col name="comment" length="256"/>
	</columns>
</table>
