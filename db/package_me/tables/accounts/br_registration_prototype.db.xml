<?xml version="1.0"?>
<!--
	[dbo].[br_registration_prototype]
	Defines prototypes for master account registration types.
-->
<table name="br_registration_prototype">
	<columns>
		<col name="registration_id" id="true" ref="br_registration"/>
		<col name="ordinal" type="int" id="true" check=">0"/>
		<col name="registration_role_id" ref="br_registration_role" required="true"/>
		<col name="natural_owner_type_id" ref="br_natural_owner_type" required="true"/>
		<col name="min_count" type="int" required="true"/>
		<col name="max_count" type="int" required="true"/>
		<col name="identification_code" type="byte" required="true" check="0..2"/>
	</columns>
</table>
