<?xml version="1.0"?>
<!--
	[dbo].[br_title_rule]
	Defines rules for master account title autogeneration.
-->
<table name="br_title_rule">
	<columns>
		<col name="title_rule_id" id="true"/>
		<col name="registration_id" ref="br_registration"/>
		<col name="sequence" type="int" required="true" unique="true"/>
		<col name="is_mandatory" required="true" default="true"/>
		<col name="prefix" length="128"/>
		<col name="suffix" length="128"/>
		<col name="repeat_prefix" length="128"/>
		<col name="title_rule_element_id" ref="br_title_rule_element"/>
		<col name="end_line" type="int"/>
		<col name="end_line_repeat" type="int"/>
	</columns>
</table>
