<?xml version="1.0"?>
<!--
	[dbo].[br_title_rule_element]
	Defines metadata elements for master account title rules.
-->
<table name="br_title_rule_element">
	<columns>
		<col name="title_rule_element_id" id="true"/>
		<col name="registration_role_id" ref="br_registration_role"/>
		<col name="interested_party_type_id" ref="br_interested_party_type"/>
		<col name="expression" length="256" required="true" default="DisplayName"/>
		<col name="description" length="128" required="true"/>
	</columns>
</table>
