<?xml version="1.0"?>
<!--
	[dbo].[br_interested_party_type]
	Defines available master account interested party types.
-->
<table name="br_interested_party_type">
	<columns>
		<col name="interested_party_type_id" id="true"/>
		<col name="name" length="256" required="true"/>
		<col name="description"/>
		<col name="tax_id_required_level" dbtype="smallint" required="true" check="-1..1"/>
		<col name="birth_date_required_level" dbtype="smallint" required="true" check="-1..1"/>
		<col name="name_required_level" dbtype="smallint" required="true" check="-1..1"/>
		<col name="address_required_level" dbtype="smallint" required="true" check="-1..1"/>
		<col name="title_required_level" dbtype="smallint" required="true" check="-1..1"/>
		<col name="disclosures_required_level" dbtype="smallint" required="true" check="-1..1"/>
		<col name="has_legal_age_required_level" dbtype="smallint" required="true" check="-1..1"/>
		<col name="interested_party_metadata" type="xml"/>
	</columns>
</table>
