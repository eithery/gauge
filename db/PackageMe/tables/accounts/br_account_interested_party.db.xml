<?xml version="1.0"?>
<!--
	[dbo].[br_account_interested_party]
	Contains interested party info for master accounts.
-->
<table name="br_account_interested_party">
	<columns>
		<col id="true"/>
		<col name="master_account_id" ref="br_master_account" required="true"/>
		<col name="interested_party_type_id" ref="br_interested_party_type" required="true"/>
		<col name="ordinal" type="byte" required="true"/>
		<col name="tax_id" length="16"/>
		<col name="birth_date" type="datetime"/>
		<col name="first_name" length="256"/>
		<col name="middle_name" length="256"/>
		<col name="last_name" length="256" index="true"/>
		<col name="designation" length="16"/>
		<col name="title" length="256"/>
		<col name="is_finra_member"/>
		<col name="is_senior_political_figure"/>
		<col name="has_legal_age"/>
		<col name="address_id" ref="br_address"/>
		<timestamps/>
	</columns>
</table>
