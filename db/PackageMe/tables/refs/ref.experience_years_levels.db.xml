<?xml version="1.0"?>
<!--
	[ref].[experience_years_levels]
	Contains available values for customer investment experience in the number of years levels.
-->
<table name="experience_years_levels" schema="ref">
	<columns>
		<col name="name" length="256" required="true" unique="true"/>
		<col name="display_name" length="256" required="true"/>
		<col name="ordinal" type="int" required="true" check=">0" unique="true"/>
		<col name="is_enabled" required="true" default="true"/>
		<col name="export_value" length="256"/>
	</columns>
</table>
