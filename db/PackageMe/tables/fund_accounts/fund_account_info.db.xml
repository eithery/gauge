<?xml version="1.0"?>
<!--
	[dbo].[fund_account_info]
	Contains fund account records.
-->
<table name="fund_account_info">
	<columns>
		<col name="fund_account_number" length="20" business_id="true"/>
		<col name="cusip" length="9" business_id="true" index="true"/>

		<col name="division_id" length="10"/>
		<col name="office_id" length="10" index="true"/>
		<col name="rep_id" length="10" index="true"/>

		<col name="cust_account" length="20" index="true"/>
		<col name="source_firm_code" length="10"/>
		<col name="isMaster" required="true" default="1"/>
		<col name="reg_type" length="6"/>
		<col name="stateOfSale" type="us_state"/>

		<col name="status" db_type="smallint" required="true" default="1" check="[-1..1]"/>
		<col name="statusChanged" type="datetime"/>
		<col name="statusChangedBy" length="256"/>
		<col name="changeStatusReasonId" ref="accountActivationReasons"/>
		<col name="purgeDate" type="datetime"/>

		<col name="network_status" type="char"/>
		<col name="network_status_date" type="datetime"/>
		<col name="network_command" type="char"/>
		<col name="network_command_date" type="datetime"/>
		<col name="network_command_source" length="10"/>
		<col name="far_command" length="3"/>
		<col name="far_command_source" length="256"/>
		<col name="far_command_date" type="datetime"/>

		<col name="group_plan_name" length="40"/>
		<col name="group_plan_alpha" length="20"/>
		<col name="group_plan_ssn" length="20"/>

		<col name="last_position_value" type="money"/>
		<col name="last_position_date" type="datetime"/>
		<col name="last_position_source" length="10"/>
		<col name="last_commission_date" type="datetime"/>

		<col name="fat_established" type="datetime"/>
		<col name="lives_credit_date" type="datetime"/>
		<col name="comment"/>
		<timestamps/>
	</columns>
</table>
