<?xml version="1.0"?>
<!--
  [dbo].[repSplitDetails]
  Contains detail info of rep splits.
-->
<table name="repSplitDetails">
  <columns>
    <col name="repSplitId" ref="repSplits" required="true"/>
    <col name="startDate"/>
    <col name="endDate"/>
    <col name="productTypeId" ref="productTypeCategories"/>
    <col name="accountNumber" length="20"/>
    <col name="baseCode" length="10"/>
    <col name="description"/>
  </columns>
</table>
