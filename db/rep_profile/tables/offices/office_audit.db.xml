<?xml version="1.0"?>
<!--
  [dbo].[officeAudit]
  Contains office audit records.
-->
<table name="officeAudit">
  <columns>
    <col name="officeId" ref="offices"/>
    <col name="repId" ref="primaryReps"/>
    <col name="auditDate" required="true"/>
    <col name="auditCompleteDate"/>
    <col name="auditSentDate"/>
    <col name="responseDate"/>
    <col name="notes"/>
  </columns>
</table>
