<?xml version="1.0"?>
<!--
  [dbo].[repContinuingEducations]
  Contains continuing education records for reps.
-->
<table name="repContinuingEducations">
  <columns>
    <col name="repId" ref="primaryReps"/>
    <col name="typeId" ref="continuingEducationTypes" required="true"/>
    <col name="officeId" ref="offices"/>
    <col name="year" type="int"/>
    <col name="requirement" type="bool"/>
    <col name="session_type" length="256"/>
    <col name="session_status" type="enum" check="0..2"/>
    <col name="window_start" type="datetime"/>
    <col name="window_end" type="datetime"/>
    <col name="scheduled" type="datetime"/>
    <col name="first_noticed" type="datetime"/>
    <col name="second_noticed" type="datetime"/>
    <col name="third_noticed" type="datetime"/>
    <col name="continuingDate"/>
    <col name="inactiveDate"/>
    <col name="terminated" type="datetime"/>
    <col name="notes"/>
  </columns>
</table>
