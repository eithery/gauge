<?xml version="1.0"?>
<!--
  [dbo].[repStatusHistory]
  Contains rep status history records.
-->
<table name="repStatusHistory">
  <columns>
    <col name="repId" ref="primaryReps" index="true"/>
    <col name="status" ref="repStatuses" required="true" check="1..255"/>
    <col name="startDate"/>
    <col name="endDate"/>
    <col name="stopPaymentDate"/>
    <col name="changeReason" ref="repTerminateReasons"/>
    <col name="changeDate"/>
    <col name="notes"/>
    <col name="updated" type="datetime"/>
    <col name="updatedBy" length="256"/>
  </columns>
</table>
